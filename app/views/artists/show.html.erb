<div class="page-header">
  <h1><%= @artist.artist_name %></h1>
</div>
<div class="col-md-8">
    <% @artist.songs.each do |song| %>
        <ul>
            <li><%= song.title %>
            <%= link_to "Delete", artist_song_path( @artist, song ), class: "btn btn-default", method: :delete, data: { confirm: "Are you sure?" } %></li>
        </ul>
    <% end %>
</div>

<div class="col-md-8">
  <%= render 'songs/form' %>
</div>
